{
    "author": "GMV Syncromatics Engineering",
    "classifications": [
        "DX/Common"
    ],
    "name": "GMV Syncromatics Engineering developer experience documents",
    "identity": "Syncromatics.Engineering.DX.Common",
    "shortName": "syncdx-common",
    "tags": {
        "language": "markdown"
    },
    "sourceName": "Syncromatics.Engineering.DX.Common",
    "preferNameDirectory": "false",
    "symbols": {
        "projectName": {
            "type": "parameter",
            "replaces": "$projectName",
            "isRequired": true
        },
        "authorName": {
            "type": "parameter",
            "defaultValue": "GMV Syncromatics Engineering",
            "replaces": "$authorName"
        },
        "authorUrl": {
            "type": "parameter",
            "defaultValue": "https://github.com/syncromatics",
            "replaces": "$authorUrl"
        },
        "authorLogo": {
            "type": "parameter",
            "defaultValue": "https://secure.gravatar.com/avatar/645145afc5c0bc24ba24c3d86228ad39?size=16",
            "replaces": "$authorLogo"
        },
        "contactEmail": {
            "type": "parameter",
            "defaultValue": "open-source@gmvsync.com",
            "replaces": "$contactEmail"
        },
        "copyrightYear": {
            "type": "generated",
            "generator": "now",
            "replaces": "$copyrightYear",
            "parameters": {
                "format": "yyyy"
            }
        },
        "githubOwner": {
            "type": "parameter",
            "defaultValue": "syncromatics",
            "replaces": "$githubOwner"
        },
        "githubRepo": {
            "type": "parameter",
            "replaces": "$githubRepo",
            "isRequired": true
        },
        "dockerOwner": {
            "type": "parameter",
            "defaultValue": "",
            "replaces": "$dockerOwner"
        },
        "dockerRepo": {
            "type": "parameter",
            "defaultValue": "",
            "replaces": "$dockerRepo"
        },
        "npmPackage": {
            "type": "parameter",
            "defaultValue": "",
            "replaces": "$npmPackage"
        },
        "nugetPackage": {
            "type": "parameter",
            "defaultValue": "",
            "replaces": "$nugetPackage"
        },
        "defaultCodeOwners": {
            "type": "@syncromatics/engineers",
            "defaultValue": "",
            "replaces": "$defaultCodeOwners"
        }
    }
}
